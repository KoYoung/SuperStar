<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    <mapper namespace="com.dao.RightDao">
    	<select id="findAllRight" resultType="com.entity.Right">
    		select * from right
    	</select>
    	<insert id="addRight" parameterType="com.entity.Right">
    		insert into right values(#{rightId,jdbcType=VARCHAR},#{rightName,jdbcType=VARCHAR},#{rightUrl,jdbcType=VARCHAR},#{rightParent,jdbcType=VARCHAR},#{rightFlag,jdbcType=VARCHAR})
    	</insert>
    </mapper>